<div class="bs-docs-section">
  <div class="row">
    <div class="col-lg-12">
      <div class="well">
        <%= link_to 'Nuevo inmueble', new_inmueble_path, :class => "btn btn-primary" %>
        <h1>Listado de inmuebles</h1>

        <table>
          <tr>
            <th>codigo de inmueble</th>
            <th>clase inmueble</th>
            <th>zona</th>
          </tr>

        <% @inmuebles.each do |inmueble| %>
          <tr>
            <td><%= link_to 'Ver', inmueble %></td>
            <td><%= link_to 'Editar', edit_inmueble_path(inmueble) %></td>
            <td><%= link_to 'Eliminar', inmueble, method: :delete, data: { confirm: '¿Está seguro?' } %></td>
          </tr>
        <% end %>
        </table>

        <br />

      </div>
    </div>
  </div>
</div>