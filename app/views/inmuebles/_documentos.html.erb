<div class="col-xs-12">
  <%= (f.object.documento_url.nil?) ? "-" : (image_tag f.object.documento_url, width: 100, height: 100) %>
  <%= f.label :documento, "#{doc_type.humanize}", :class => "lead report-field" %>
  <br>
  <%= f.object.documento.file.original_filename unless f.object.documento.file.nil? %>
  <%= f.file_field :documento, :class => :upload, :accept=> "*"%>
  <%= f.hidden_field(:descripcion, value: "#{doc_type}", :class => "form-control") %>
  <hr>
</div>

