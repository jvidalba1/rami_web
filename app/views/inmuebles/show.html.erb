	<div class="bs-docs-section">
	<div class="row">
		<div class="col-lg-12">
			<%# image_tag @inmueble.logo_url(:thumb).to_s %>
			<div class="well">
				<%= link_to "Atrás", :back, :class => "btn btn-default"%>
        <%= link_to "Editar", edit_inmueble_path(@inmueble), :class => "btn btn-primary"%>
        <%= link_to "Reporte", format_pdf_inmuebles_path(@inmueble.id), :class => "btn btn-primary", :target => "_blank" %>
        <br>
        <br>
				<div class="bs-example">
				  <ul class="nav nav-tabs" style="margin-bottom: 15px;">
				    <li class="active">
				    	<a href="#datos" id="tab_datos" data-toggle="tab">Datos</a>
				    </li>
				    <li class="">
				    	<a href="#propietario" id="tab_propietario" data-toggle="tab">Propietario</a>
				    </li>
				    <li class="">
				    	<a href="#interesado" id="tab_interesado" data-toggle="tab">Interesados/Inversionistas</a>
				    </li>
				    <li class="">
              <a href="#intermediario" id="tab_intermediario" data-toggle="tab">Intermediarios</a>
            </li>
            <li class="">
              <a href="#documentos" id="tab_documentos" data-toggle="tab">Documentos/Imágenes</a>
            </li>

				  </ul>
				  <div id="myTabContent" class="tab-content container-fluid">
				    <div class="tab-pane fade active in" id="datos">
				      <table class="table table-hover">
				      	<tbody>
				      		<tr>
				      			<th>Nombre inmueble</th>
				      			<td><%= @inmueble.nombre_inmueble %></td>
				      			<th>Dirección</th>
				      			<td><%= @inmueble.direccion %></td>
				      			<th>Zona</th>
				      			<td><%= @inmueble.zona %></td>
				      		</tr>
				      		<tr>
				      			<th>Clase de inmueble</th>
				      			<td><%= get_clase_inmueble @inmueble.clase_inmueble %></td>
				      			<th>Tipo de negocio</th>
				      			<td><%= get_tipo_negocio @inmueble.tipo_negocio %></td>
				      			<th>Ciudad</th>
				      			<td><%= @inmueble.ciudad.try(:nombre) %></td>
				      		</tr>
				      		<tr>
				      			<th>Área disponible</th>
				      			<td><%= @inmueble.area_disponible %> mt2</td>
				      			<th>Estado</th>
				      			<td><%= get_status(@inmueble.status) unless @inmueble.status.nil? %></td>
				      			<th>Número de pisos</th>
				      			<td><%= @inmueble.nro_pisos %></td>
				      		</tr>
				      		<tr>
				      			<th>Valor m2</th>
				      			<td><%= number_to_currency @inmueble.valor_mt2 %></td>
				      			<th>Valor total</th>
				      			<td><%= number_to_currency @inmueble.valor_total %></td>
				      			<th>Valor administración</th>
				      			<td><%= number_to_currency @inmueble.valor_admin %></td>
				      		</tr>
				      		<tr>
				      			<th>Predial</th>
				      			<td><%= number_to_currency @inmueble.predial %></td>
				      			<th>Fecha captado</th>
				      			<td><%= l @inmueble.fecha_captado, format: :long unless @inmueble.fecha_captado.nil? %></td>
				      			<th>Número pisos permitidos</th>
				      			<td><%= @inmueble.nro_pisos_permitidos %></td>
				      		</tr>
				      		<tr>
				      			<th>Altura</th>
				      			<td><%= @inmueble.altura %> metros</td>
				      			<th>Fondo</th>
				      			<td><%= @inmueble.fondo %> metros </td>
				      			<th>Frente</th>
				      			<td><%= @inmueble.frente %> metros</td>
				      		</tr>
				      		<tr>
				      			<th>Fecha disponibilidad</th>
				      			<td><%= l @inmueble.fecha_disponibilidad, format: :long unless @inmueble.fecha_disponibilidad.nil? %></td>
				      			<th>Fecha salida inventario</th>
				      			<td><%= l @inmueble.fecha_salida_inventario, format: :long unless @inmueble.fecha_salida_inventario.nil?%></td>
				      			<th>Año de construcción</th>
				      			<td><%= @inmueble.anio_construccion %></td>
				      		</tr>

				      		<tr>
				      			<th>Índice ocupación</th>
				      			<td><%= number_to_percentage @inmueble.indice_ocupacion %></td>
				      			<th>Índice construcción</th>
				      			<td><%= number_to_percentage @inmueble.indice_construccion %></td>
				      			<th>Porcentaje comisión</th>
				      			<td><%= number_to_percentage @inmueble.porcentaje_comision %></td>
				      		</tr>
				      		<tr>
				      			<th>Parqueaderos</th>
				      			<td><%= @inmueble.parqueadores %></td>
				      			<th>Densidad</th>
				      			<td><%= @inmueble.densidad %></td>
				      			<th></th>
				      			<td></td>
				      		</tr>
				      		<tr>
				      			<th>Descripción área disponible</th>
				      			<td colspan="6"><%= @inmueble.area_disponible_desc %></td>
				      		</tr>
				      		<tr>
				      			<th>Entorno</th>
				      			<td colspan="6"><%= @inmueble.entorno %></td>
				      		</tr>
				      		<tr>
				      			<th>Otras características</th>
				      			<td colspan="6"><%= @inmueble.otras_caracteristicas %></td>
				      		</tr>
				      		<tr>
				      			<th>Transporte</th>
				      			<td colspan="6"><%= @inmueble.transporte %></td>
				      		</tr>
				      	</tbody>
				      </table>
				    </div>
				    <!--
				    	Tab for propietario
				  	-->
				    <div class="tab-pane fade" id="propietario">
					    <div class="bs-example table-responsive">
					    	<h1>Propietario</h1>
					    	<% if @inmueble.propietario.nil? %>
					    		<h3>No aplica</h3>
					    	<% else %>
					    		<table class="table table-striped table-hover ">
										<thead>
									      	<tr>
									      		<th>Nombre propietario</th>
									      		<th>Telégono</th>
									      		<th>Celular</th>
									      		<th>Email</th>
									      		<th>Dirección</th>
									      		<th colspan="2">Opciones</th>
									      	</tr>
										</thead>
							      	<tbody>
							      		<tr>
							      			<td><%= @inmueble.propietario.nombre %></td>
							      			<td><%= @inmueble.propietario.telefono %></td>
							      			<td><%= @inmueble.propietario.celular %></td>
							      			<td><%= @inmueble.propietario.email %></td>
							      			<td><%= @inmueble.propietario.direccion %></td>
							      			<td>
									          	<div class="bs-example">
										          <div class="btn-group" >
										            <%= link_to 'Editar', "#", :class => "btn btn-default" %>
										          </div>
										        </div>
									        </td>
							      		</tr>
							      	</tbody>
								  </table>
					    	<% end %>
						  </div>
				    </div>
				    <!--
				    	Tab for interesados
				  	-->
				    <div class="tab-pane fade" id="interesado">
				    	<div class="bs-example table-responsive">
				    		<h1>Interesados/Inversionistas</h1>
				    		<% unless @inmueble.interesados.empty? %>
									<table class="table table-striped table-hover ">
										<thead>
							      	<tr>
							      		<th>Nombre</th>
							      		<th>Teléfono</th>
							      		<th>Celular</th>
							      		<th>Email</th>
							      	</tr>
										</thead>
										<% @inmueble.interesados.each do |interesado| %>
											<% unless interesado.nombre.eql? "" %>
								      	<tbody>
								      		<tr>
								      			<td><%= interesado.nombre %></td>
								      			<td><%= interesado.telefono %></td>
								      			<td><%= interesado.celular %></td>
								      			<td><%= interesado.email %></td>
								      		</tr>
								      	</tbody>
								      <% end %>
							      <% end %>
					  			</table>
					  		<% else %>
					  			<h4>No hay interesados para este inmueble</h4>
					  		<% end %>
					  	</div>
				    </div>
				    <!--
				    	Tab for intermediarios
				  	-->
				    <div class="tab-pane fade" id="intermediario">
				    	<div class="bs-example table-responsive">
								<h1>Intermediarios </h1>
			    			<% unless @inmueble.intermediarios.empty? %>
			    				<table class="table table-striped table-hover ">
										<thead>
							      	<tr>
							      		<th>Nombre</th>
							      		<th>Teléfono</th>
							      		<th>Email</th>
							      		<th>Última fecha de contacto</th>
							      	</tr>
										</thead>
										<% @inmueble.intermediarios.each do |intermediario| %>
											<% unless intermediario.nombre.eql? "" %>
								      	<tbody>
								      		<tr>
								      			<td><%= intermediario.nombre %></td>
								      			<td><%= intermediario.telefono %></td>
								      			<td><%= intermediario.email %></td>
								      			<td><%= l intermediario.ult_fecha_contacto, format: :long unless intermediario.ult_fecha_contacto.nil? %></td>
								      		</tr>
								      	</tbody>
								      <% end %>
							      <% end %>
							    </table>
						    <% else %>
									<h4>No hay intermediarios para este inmueble</h4>
						    <% end %>
					  	</div>
					  </div>
					  <!--
				    	Tab for documentos
				  	-->
            <div class="tab-pane fade" id="documentos">
              <div class="bs-example table-responsive">
              	<h1>Documentos adjuntos </h1>
			    			<% if @inmueble.documentos %>
                  <table class="table table-striped table-hover ">
                      <% @inmueble.documentos.each do |doc| %>
                      	<% if doc.documento_url %>
	                        <tbody>
	                          <tr>
	                            <td>
	                              <%= doc.descripcion %>
	                            </td>
	                            <td>
	                              <%= link_to "Descargar", doc.documento_url %>
	                            </td>
	                          </tr>
	                        </tbody>
	                    	<% end %>
                      <% end %>
                  </table>
                <% else %>
                	<h4>No hay documentos para este inmueble</h4>
                <% end %>
              </div>
            </div>
				  </div>
				</div>
			</div>
		</div>
	</div>
</div>
